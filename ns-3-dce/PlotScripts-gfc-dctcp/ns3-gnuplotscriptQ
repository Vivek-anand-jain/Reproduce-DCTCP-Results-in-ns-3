set xrange [3:10]
set yrange [0:700]
set terminal pngcairo enhanced color lw 2 size 800,600 font 'Arial-Bold'
set output "queue-0.png"
set xlabel "Time (sec)"
set ylabel "Queue occupancy (No. of packets)"
set key outside;
set key right top;
plot "queue-0.plotme" title "T1" with lines lw 1.5

